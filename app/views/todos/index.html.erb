<section class="todoapp">
  <header class="header">
    <h1>todos</h1>
    <input class="new-todo" placeholder="What needs to be done?" autofocus data-behavior="create">
  </header>

  <%= render partial: "/todos/main" %>
  <%= render partial: "/todos/footer" %>
</section>

<footer class="info">
